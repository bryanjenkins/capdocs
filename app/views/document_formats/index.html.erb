<h1>Listing Document Formats</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Symbol</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @document_formats.each do |document_format| %>
  <tr>
    <td><%= document_format.name %></td>
    <td><%= document_format.symbol %></td>
    <td><%= link_to 'Show', document_format %></td>
    <td><%= link_to 'Edit', edit_document_format_path(document_format) %></td>
    <td><%= link_to 'Destroy', document_format, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

